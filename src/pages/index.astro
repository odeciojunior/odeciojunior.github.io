---
/**
 * Root index page that redirects to the default locale with prefix
 * Required when prefixDefaultLocale: true is set in astro.config.ts
 */
import { LOCALE_PATHS, DEFAULT_LANGUAGE } from "@/i18n/config";

// Perform server-side redirect to the default language prefix ("/en")
return Astro.redirect(LOCALE_PATHS[DEFAULT_LANGUAGE]);
